{
  "name": "test-count-up",
  "description": "this app isn't distributed, it just used to test ngx-countUp",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "build:countup": "ng-packagr -p projects/count-up/ng-package.json",
    "pack:countup": "cd dist/count-up && yarn pack && cd ../..",
    "package:countup": "yarn build:countup && yarn pack:countup",
    "install-tarball": "yarn add ./dist/count-up/ngx-countUp-v7.3.2.tgz",
    "all": "yarn package:countup && yarn install-tarball",
    "reset:countup": "yarn remove ngx-countUp",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:watch": "jest --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "~8.0.0",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@briebug/jest-schematic": "^1.6.0",
    "rxjs": "~6.5.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.9",
    "@angular-devkit/build-ng-packagr": "~0.803.21",
    "@angular/cli": "~8.3.9",
    "@angular/compiler-cli": "~8.2.9",
    "@angular/language-service": "~8.2.9",
    "@types/jasmine": "~3.3.13",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~12.0.4",
    "codelyzer": "~5.0.1",
    "countup.js": "^2.0.6",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^24.7.1",
    "jest-preset-angular": "7.1.1",
    "ng-packagr": "^5.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~8.2.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.17.0",
    "typescript": "~3.5.3"
  }
}
